create table 'blog' (
  'id' int(5) not null,
  'jid' varchar(50) not null,
  'content' text not null,
  'title' varchar(150) not null,
  'date' datetime not null,
  'tags' text not null,
  primary key  ('id')
);

create table 'quotes' (
  'id' int(5) not null,
  'content' text not null,
  'channel' varchar(50) not null,
  'date' datetime not null,
  primary key  ('id')
);

create table 'status' (
  'id' int(5) not null,
  'jid' varchar(50) not null,
  'status' int(5) not null,
  primary key  ('id')
);

create table if not exists 'rss_feeds' (
  'id' int(5) not null,
  'rss_url' text not null,
  'title' text not null,
  primary key  ('id')
);
